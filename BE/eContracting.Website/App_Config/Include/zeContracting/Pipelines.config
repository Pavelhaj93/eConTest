<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <pipelines>
            <initialize>
                <processor type="eContracting.Website.Pipelines.Initialize.RegisterApiProcessor, eContracting.Website" patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeRoutes, Sitecore.Mvc']" />
            </initialize>
            <sessionEnd>
                <processor type="eContracting.Website.Pipelines.SessionEnd.ClearUserCacheProcessor, eContracting.Website" />
            </sessionEnd>
            <getTranslation>
                <processor type="eContracting.Website.Pipelines.GetTranslation.CheckTranslationProcessor, eContracting.Website" />
            </getTranslation>
        </pipelines>
        <events>
            <event name="publish:end">
                <handler type="eContracting.Website.Events.PublishEnd.ClearDictionaryCacheHandler, eContracting.Website" method="Run" />
            </event>
            <event name="publish:end:remote">
                <handler type="eContracting.Website.Events.PublishEnd.ClearDictionaryCacheHandler, eContracting.Website" method="Run" />
            </event>
        </events>
    </sitecore>
</configuration>
