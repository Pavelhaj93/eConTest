<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <services>
            <register serviceType="eContracting.ILogger, eContracting.Core" implementationType="eContracting.SitecoreLogger, eContracting.Core" />
            <register serviceType="eContracting.IApiService, eContracting.Core" implementationType="eContracting.Services.CacheApiService, eContracting.Services" />
            <register serviceType="eContracting.IOfferParserService, eContracting.Core" implementationType="eContracting.Services.OfferParserService, eContracting.Services" />
            <register serviceType="eContracting.IOfferAttachmentParserService, eContracting.Core" implementationType="eContracting.Services.OfferAttachmentParserService, eContracting.Services" />
            <register serviceType="eContracting.IAuthenticationService, eContracting.Core" implementationType="eContracting.Services.AuthenticationService, eContracting.Services" />
            <register serviceType="eContracting.IAuthenticationProviderService, eContracting.Core" implementationType="eContracting.Services.SessionAuthenticationProviderService, eContracting.Services" />
            <register serviceType="eContracting.ISettingsReaderService, eContracting.Core" implementationType="eContracting.Services.SitecoreSettingsReaderService, eContracting.Services" />
            <register serviceType="eContracting.ILoginReportStore, eContracting.Core" implementationType="eContracting.Services.MongoLoginReportStore, eContracting.Services" />
            <register serviceType="Glass.Mapper.Sc.ISitecoreContext, Glass.Mapper.Sc" implementationType="Glass.Mapper.Sc.SitecoreContext, Glass.Mapper.Sc" />
            <register serviceType="eContracting.Kernel.Utils.IAuthenticationDataSessionStorage, eContracting.Kernel" implementationType="eContracting.Kernel.Utils.AuthenticationDataSessionStorage, eContracting.Kernel" />
        </services>
    </sitecore>
</configuration>